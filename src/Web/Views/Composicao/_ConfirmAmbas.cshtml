<div class="modal" id="ModalConfirmAmbas" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-dialog">
        @Html.Hidden("idComposicaoChecklist");
        <div class="modal-content" style="width:40%; margin: 10px auto">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
            </div>
            <div class="modal-body">
                <i class="fa fa-4x fa-exclamation-triangle" id="iconeModel" style="float: left; color: rgb(255, 190, 0); margin-top: 5px;"></i>
                <h4 id="MessageConfirm1">Deseja replicar o checklist para EAB?</h4>
            </div>
            <div class="modal-footer">
                <button class="btn btn-custom" type="button" onclick="raizenCoreJs.Composicao.SalvarConfirmacaoCheckList($('#idComposicaoChecklist').val(), true);">Sim</button>
                <button class="btn btn-custom" type="button" onclick="raizenCoreJs.Composicao.SalvarConfirmacaoCheckList($('#idComposicaoChecklist').val(),false);">Não</button>
            </div>
        </div>
    </div>
</div>