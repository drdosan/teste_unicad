<script type="text/javascript" language="javascript">
    jQuery(document).ready(function () {        
        App.init(); //Initialise plugins and elements
        raizenCoreJs.raizenMenus.UrlGetItemMenu = "@urlGetitemMenu";
        raizenCoreJs.raizenMenus.UrlSetItemMenu = "@urlSetitemMenu";
        raizenCoreJs.raizenMenus.ManterPosicaoMenu();
        raizenCoreJs.raizenMask.ConfigurarMaskaras();
        $.ajaxSetup({
            cache: false,
            statusCode: {
                350: function (jqXHR, textStatus, errorThrown) {
                    var response = $.parseJSON(jqXHR.responseText);
                    if (response != null && response.ssoResponse == true) {
                        window.document.location.href = response.redirectUrl;
                    }
                }
            }
        });
        raizenCoreJs.raizenHelpers.FecharLoading();
        
    });

</script>


